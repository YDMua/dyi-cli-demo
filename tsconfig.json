{
  "compilerOptions": {
    "target": "es6", 
    "module": "commonjs", 
    "outDir": "./lib", 
    "baseUrl": "./src",
    "strict": true, 
    "moduleResolution": "node",
    "esModuleInterop": true, 
    "resolveJsonModule": true,
    "skipLibCheck": true, 
    "forceConsistentCasingInFileNames": true, 
    "typeRoots": ["./typings"]
  },
  "include": [
    "./src",
  ]
}
